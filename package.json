{
    "name": "expando-vscode",
    "displayName": "Expando for Visual Studio Code",
    "description": "Create instant data snippets",
    "version": "0.0.1",
    "publisher": "Fanie Reynders",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:expando.expand"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "expando.expand",
            "title": "Run Expando on selection"
        }],
        "keybindings": [
            {
                "command": "expando.expand",
                "key": "shift+enter",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "expando-core":">=0.0.4"
    }
}